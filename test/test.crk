fun foo (u : Int ; v : Bool) : Int = u

fun bar (u : Int ; v : Bool) : Bool = v

fun baz (u : Int ; v : Bool) : Int . Bool = (u ; v)

fun qz (z : Int . Bool) : Int . Bool =
    let (x;y) = z in (x;y)

fun bot (x : Int + Bool ; y : Bool) : (Int . Bool) + (Bool . Bool) =
    case x of
      inl x' => inl (x' ; y)
    | inr x' => inr (x' ; y)
    
exec bot (emp)
exec bot (inl(emp))
exec bot (inl(3))
exec bot (inl(3);false)

fun delaycase(y : Bool; x : Int + Bool) : (Bool . Int) + (Bool . Bool) =
  case x of
    inl x' => inl (y ; x')
  | inr x' => inr (y ; x')

exec delaycase (emp)
exec delaycase (false)
exec delaycase (false;inl(emp))
exec delaycase (false;inl(3))